<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>msf_EsRunningUser__c</fullName>
    <description>Campo que calcula si el campo &apos;Created by&apos; está asignado al usuario que está ejecutando la sesión</description>
    <externalId>false</externalId>
    <formula>IF( CreatedById  = $User.Id , true , false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Es Running User</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
