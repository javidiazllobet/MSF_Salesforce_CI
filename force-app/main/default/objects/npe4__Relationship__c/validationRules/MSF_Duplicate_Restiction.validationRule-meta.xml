<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MSF_Duplicate_Restiction</fullName>
    <active>true</active>
    <description>An organizational contact can not be duplicated from an individual contact and vice versa.</description>
    <errorConditionFormula>ISPICKVAL( npe4__Type__c , &apos;Duplicate&apos;) &amp;&amp;
( 
  (npe4__Contact__r.RecordType.DeveloperName = &apos;Organization_Contact&apos; &amp;&amp;  npe4__RelatedContact__r.RecordType.DeveloperName &lt;&gt; &apos;Individual_Contact&apos;) ||
  (npe4__Contact__r.RecordType.DeveloperName = &apos;Individual_Contact&apos; &amp;&amp;  npe4__RelatedContact__r.RecordType.DeveloperName &lt;&gt; &apos;Organization_Contact&apos;) 
)</errorConditionFormula>
    <errorDisplayField>npe4__Type__c</errorDisplayField>
    <errorMessage>Este tipo de relaci√≥n no esta permitido entre los tipos de los contactos seleccionados.</errorMessage>
</ValidationRule>
