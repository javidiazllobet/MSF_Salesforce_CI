<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>53.0</apiVersion>
    <description>Actualiza las opps (trustly, paypal y tc-online) de comprometidas a cancel (&lt; yesterday)
El usuario que queda como last_modify_user es: Integration Marketing Cloud Tracking</description>
    <interviewLabel>update_opps_online_from_comprometidas_to_cancel {!$Flow.CurrentDateTime}</interviewLabel>
    <label>update_opps_online_from_comprometidas_to_cancel_less_yesterday</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>update_from_comprometida_to_cancel</name>
        <label>update_from_comprometida_to_cancel</label>
        <locationX>811</locationX>
        <locationY>129</locationY>
        <filterLogic>(1 or 2 or 3) and 4 and 5</filterLogic>
        <filters>
            <field>npsp4hub__Payment_Method__c</field>
