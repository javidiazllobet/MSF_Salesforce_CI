        <filters>
            <field>npsp4hub__Payment_Method__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>TC ONLINE</stringValue>
            </value>
        </filters>
        <filters>
            <field>npsp4hub__Payment_Method__c</field>
            <operator>EqualTo</operator>
